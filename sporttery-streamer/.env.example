# Redis
REDIS_HOST=redis-xxxx.example.com
REDIS_PORT=6379
REDIS_USER=default
REDIS_PASS=your_password
REDIS_TLS=false

# Browser discovery (same network host or exposed host IP)
CHROME_DISCOVERY=http://chrome:9222/json/version

# Scheduling
ODDS_CRON=*/3 11-21 * * *
JITTER_SEC_MAX=90

# History/TTL
ODDS_MAXLEN=300
ODDSEXPIRE_H=72

# Logging
LOG_LEVEL=info
